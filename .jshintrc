{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 3,
    "maxdepth": 3,
    "maxlen": 100,

    "eqnull": true,
    "laxcomma": true,
    "sub": true,

    "node": true,

    "white": true,

    "globals": {
        "define": true,
        "require": true
    }
}
